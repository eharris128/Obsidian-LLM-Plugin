{
	"name": "llm-plugin",
	"version": "0.19.7",
	"description": "This plugin allows users to access LLMs downloaded from the GPT4ALL chat client for use inside Obsidian notes as well as LLMs from OpenAI and Gemini",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.27.3",
		"@google/generative-ai": "^0.8.0",
		"openai": "^4.53.0"
	}
}
